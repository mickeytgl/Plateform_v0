<title>Plateform</title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<%= csrf_meta_tags %>
<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyBpucAVkBL9g3iZpzfCb3VkJzSh2vXIdck&libraries=places", "https://js.stripe.com/v3/", "application", 'data-turbolinks-track': 'reload' %>
